this.showDesc=function(desc)
{if(this.options.caption)
{if(desc.hasClass('s-c-l')||desc.hasClass('s-c-c')||desc.hasClass('s-c-r'))
{desc.css('top',obj.r.h/2-desc.outerHeight()/2+"px");}
