function spider_check_email(id){if(jQuery('#'+id).val()!=''){var email=jQuery('#'+id).val().replace(/^\s+|\s+$/g,'');if(email.search(/^\w+((-\w+)|(\.\w+))*\@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z0-9]+$/)==-1){alert(bwg_objectL10n.bwg_mail_validation);return true;}
return false;}}
